<h1 class="text-center">Products</h1>
<section id="products">
  <div class="row row-eq-height">
    <div class="col-md-12">   
      <% if @products.any? %>
        <%= render @products %>
      <% else %>
        <div class="well">
          No products yet
        </div>
      <% end %>
    </div>
  </div>
</section>